{
  "name": "file-processor",
  "version": "0.0.0",
  "description": "file-processor 用于批量处理文件，可自定义处理逻辑",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "bin": {
    "fileproc":"./dist/fileproc.mjs"
  },
  "sideEffects": false,
  "homepage": "https://github.com/GuoBinyong/file-processor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuoBinyong/file-processor"
  },
  "bugs": {
    "url": "https://github.com/GuoBinyong/file-processor/issues",
    "email": "guobinyong@qq.com"
  },
  "keywords": [
    "file process",
    "文件处理",
    "文本处理"
  ],
  "author": {
    "name": "郭斌勇",
    "email": "guobinyong@qq.com"
  },
  "license": "MIT",
  "files": [
    "dist/[^.]*",
    "types/[^.]*",
    "docs/[^.]*",
    "doc/[^.]*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "bunch": "vite build --mode bunch",
    "debug": "debug=true vite build",
    "debug:bunch": "debug=true vite build --mode bunch",
    "preview": "vite preview",
    "api": "api-extractor run -c api-extractor.jsonc -l -v",
    "doc": "npm run api ; api-documenter markdown -i ./temp -o doc/api"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.15",
    "@microsoft/api-extractor": "^7.24.1",
    "@types/node": "^18.11.9",
    "build-tls": "^1.3.11",
    "dts-bundle-generator": "^6.12.0",
    "package-tls": "^1.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.0"
  }
}
